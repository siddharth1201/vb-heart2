    {% endif %}
    <div class="zoom-profile-inline-form">
      <input type="text" class="edit-zoom-profile-sign_in_email" name="sign_in_email">
      <a class="save-zoom-profile-sign_in_email save-zoom-profile-field">{{ 'Save'|t }}</a>
    </div>
  </div>
  <div class="field-item">
    <label>{{ 'Sign-In Password'|t}}</label>
    {% if data.sign_in_password %}
      <span class="set-value">{{ data.sign_in_password }}</span>
      <a class="btn edit-btn">{{ 'Edit'|t }}</a>
    {% else %}
      <span class="set-value">{{ 'Not Set'|t }}</span>
      <a class="edit-zoom-profile-sign_in_password">{{ 'Add Sign-In Password'|t }}</a>
    {% endif %}
    <div class="zoom-profile-inline-form">
      <input type="text" class="edit-zoom-profile-sign_in_password" name="sign_in_password">
      <a class="save-zoom-profile-sign_in_password save-zoom-profile-field">{{ 'Save'|t }}</a>
    </div>
  </div>
  <div class="field-item">
    <label>{{ 'Two-Factor Authentication'|t}}</label>
    {% if data.authentication == true %}
      <span class="set-value">{{ 'On'|t}}</span>
      <a class="btn edit-btn">{{ 'Turn Off'|t }}</a>
    {% else %}
      <span class="set-value">{{ 'Off'|t }}</span>
      <a class="edit-zoom-profile-authentication edit-btn">{{ 'Turn On'|t }}</a>
    {% endif %}
    <div class="zoom-profile-inline-form">
      <input type="radio" class="edit-zoom-profile-authentication" name="authentication" value="1">
      <label>{{ 'On'|t}}</label>
      <input type="radio" class="edit-zoom-profile-authentication" name="authentication" value="0">
      <label>{{ 'Off'|t}}</label>
      <a class="save-zoom-profile-authentication save-zoom-profile-field">{{ 'Save'|t }}</a>
    </div>
  </div>
  <div class="field-item">
    <label>{{ 'OPT Authentication'|t}}</label>
