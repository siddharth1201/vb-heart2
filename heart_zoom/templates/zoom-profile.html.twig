      <span class="set-value">{{ data.otp }}</span>
    {% else %}
      <span class="set-value">{{ 'On'|t }}</span>
    {% endif %}
  </div>
  <div class="field-item">
    <a class="save-zoom-profile-sign-out-other-device save-zoom-profile-field">{{ 'Sign Me Out From All Devices'|t }}</a>
  </div>
</div>
